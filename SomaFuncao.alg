algoritmo "somafuncao"

var
V1, V2,S: INTEIRO

FUNCAO SOMA (X, Y: INTEIRO) :INTEIRO
inicio
      RETORNE X+Y
FIMFUNCAO

INICIO
      ESCREVA("DIGITE O PRIMEIRO VALOR: ")
      LEIA(V1)
      ESCREVA ("DIGITE O SEGUNDO VALOR: ")
      LEIA (V2)
      S<-SOMA(V1,V2)
      ESCREVA("A SOMA ENTRE ", V1, " E ", V2, " E IGUAL A ", S)
fimalgoritmo

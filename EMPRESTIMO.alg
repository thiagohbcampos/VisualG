algoritmo "semnome"

var
   EMPRESTIMO, JUROS, PARCELA: REAL

inicio
      ESCREVA ("QUAL O VALOR DO EMPRESTIMO? ")
      LEIA (EMPRESTIMO)
      ESCREVA ("EM QUANTAS PARCELAS VOCE QUER PAGAR? ")
      LEIA (PARCELA)
      JUROS <- (EMPRESTIMO *0,2)/PARCELA
      ESCREVA ("O VALOR DAS PARCELAS SERA DE ",JUROS)

fimalgoritmo
